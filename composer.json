{
  "name": "onesite/core",
  "description": "1Site Core",
  "license": "MIT",
  "authors": [
    {
      "name": "1Site.vn",
      "email": "hotro@1site.vn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.5",
    "phpseclib/phpseclib": "^2.0",
    "dflydev/dot-access-data": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "OneSite\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/helpers.php"
    ],
    "psr-4": {
      "OneSite\\Core\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x.x-dev"
    }
  }
}
